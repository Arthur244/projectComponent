J,GAAZ;;AACA,UAAID,QAAJ,EAAc;AACZI,QAAAA,IAAI,CAACE,QAAL,GAAgBN,QAAhB;AACD;;AACDI,MAAAA,IAAI,CAACG,KAAL;AACD;AAED;AACF;AACA;;;;+BACoB;AAChB,UAAMC,EAAE,GAAGC,SAAS,CAACC,SAArB;AACA,UAAMC,IAAI,GAAGH,EAAE,CAACI,KAAH,CAAS,sBAAT,CAAb;AACA,UAAMC,QAAQ,GAAG,CAACF,IAAD,IAASH,EAAE,CAACI,KAAH,CAAS,wBAAT,CAA1B;AACA,UAAME,SAAS,GAAGN,EAAE,CAACI,KAAH,CAAS,sBAAT,CAAlB;AACA,aAAOC,QAAQ,IAAIC,SAAnB;AACD;AAED;AACF;AACA;AACA;AACA;;;;6BACkBC,G,EAAsB;AACpC,UAAIC,OAAO,GAAGrB,QAAQ,CAACC,aAAT,CAAuB,UAAvB,CAAd;AACAoB,MAAAA,OAAO,CAACC,KAAR,GAAgBF,GAAhB;AACAC,MAAAA,OAAO,CAACE,KAAR,CAAcC,UAAd,GAA2B,OAA3B;AACAH,MAAAA,OAAO,CAACE,KAAR,CAAcE,KAAd,GAAsB,GAAtB;AACAJ,MAAAA,OAAO,CAACE,KAAR,CAAcG,MAAd,GAAuB,GAAvB;AACA1B,MAAAA,QAAQ,CAAC2B,IAAT,CAAcC,WAAd,CAA0BP,OAA1B;AACAA,MAAAA,OAAO,CAACQ,MAAR;AAEA,UAAIC,MAAM,GAAG,IAAb;;AACA,UAAI;AACF9B,QAAAA,QAAQ,CAAC+B,WAAT,CAAqB,MAArB;AACD,OAFD,CAEE,OAAOC,KAAP,EAAc;AACdF,QAAAA,MAAM,GAAG,KAAT;AACD,OAJD,SAIU;AACR9B,QAAAA,QAAQ,CAAC2B,IAAT,CAAcM,WAAd,CAA0BZ,OAA1B;AACD;;AACD,aAAOS,MAAP;AACD;;;;;;eAGYjC,Q;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAlEMA,Q","sourcesContent":["class HtmlUtil {\n  /**\n   * 富文本转换为纯文件\n   * @param htmlStr 富文本\n   */\n  static plainHtml(htmlStr?: string) {\n    if (!htmlStr) {\n      return '';\n    }\n    const span = document.createElement('span');\n    span.innerHTML = htmlStr;\n    return span.innerText;\n  }\n\n  /**\n   * 从blob中导出文件\n   * @param blob blob实例\n   * @param fileName 文件名\n   */\n  static exportBlob(blob: Blob, fileName?: string) {\n    let url = URL.createObjectURL(blob);\n    const link = document.createElement('a');\n    link.href = url;\n    if (fileName) {\n      link.download = fileName;\n    }\n    link.click();\n  }\n\n  /**\n   * 判断是否是移动端\n   */\n  static isMobile() {\n    const ua = navigator.userAgent;\n    const ipad = ua.match(/(iPad).*OS\\s([\\d_]+)/);\n    const isIphone = !ipad && ua.match(/(iPhone\\sOS)\\s([\\d_]+)/);\n    const isAndroid = ua.match(/(Android)\\s+([\\d.]+)/);\n    return isIphone || isAndroid;\n  }\n\n  /**\n   * 设置系统粘贴板的文字\n   * @param str 要复制的文本内容\n   * @param completeHandler 设置结束的处理函数，格式为fun(result)。 result表示复制结果，true表示复制成功，false表示复制失败\n   */\n  static copyText(str: string): boolean {\n    let element = document.createElement('textarea');\n    element.value = str;\n    element.style.visibility = 'false';\n    element.style.width = '0';\n    element.style.height = '0';\n    document.body.appendChild(element);\n    element.select();\n\n    let result = true;\n    try {\n      document.execCommand('Copy');\n    } catch (error) {\n      result = false;\n    } finally {\n      document.body.removeChild(element);\n    }\n    return result;\n  }\n}\n\nexport default HtmlUtil;\n"]}]}