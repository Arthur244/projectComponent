mProps}\n        name={name}\n        label={label}\n        rules={[\n          {\n            required,\n            message: emptyMsg || '请填写电话号码',\n          },\n          {\n            pattern: this.getPhone(phoneType),\n            message: errMsg || '电话号码格式不正确',\n          },\n        ]}\n      >\n        <Input />\n      </Form.Item>\n    );\n  }\n}\n\nexport default PhoneFormItem;\n"]}]}